{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "framework": "vite",
  "buildCommand": "vite build",
  "outputDirectory": "client/dist",
  "functions": {
    "api/index.ts": {
      "runtime": "vercel-node@latest",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
